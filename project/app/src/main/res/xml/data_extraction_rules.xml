<data-extraction-rules>
    <cloud-backup disableIfNoEncryptionCapabilities="true">
        <include domain="root" />
        <exclude domain="file" />
        <exclude domain="database" />
        <include
            domain="sharedpref"
            path="." />
        <exclude domain="external" />
    </cloud-backup>
    <device-transfer>
        <include domain="root" />
        <include domain="file" />
        <include
            domain="database"
            path="." />
        <include
            domain="sharedpref"
            path="." />
        <exclude domain="external" />
    </device-transfer>
</data-extraction-rules>
