        app:iconSpaceReserved="false">
        <intent
            android:action="android.intent.action.VIEW"
            android:data="https://github.com/objectbox/objectbox-java" />
    </Preference>
    <Preference
        android:key="okhttp"
        android:title="Okhttp3"
        android:summary="Square"
        app:iconSpaceReserved="false">
        <intent
            android:action="android.intent.action.VIEW"
            android:data="https://github.com/square/okhttp" />
    </Preference>
    <Preference
        android:key="rengwuxian-materialedittext"
        android:title="MaterialEditText"
        android:summary="Rengwuxian"
        app:iconSpaceReserved="false">
        <intent
            android:action="android.intent.action.VIEW"
            android:data="https://github.com/rengwuxian/MaterialEditText" />
    </Preference>
    <Preference
        android:key="timber"
        android:title="Timber"
        android:summary="Jacke Wharton"
        app:iconSpaceReserved="false">
        <intent
            android:action="android.intent.action.VIEW"
            android:data="https://github.com/JakeWharton/timber" />
