<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:title="@string/preferencesNotification">
    <androidx.preference.PreferenceCategory
        app:iconSpaceReserved="false"
        app:title="@string/preferencesCategoryNotificationOngoing">
        <androidx.preference.SwitchPreferenceCompat
            app:iconSpaceReserved="false"
            app:key="notificationLocation"
            app:summary="@string/preferencesNotificationLocationSummary"
            app:title="@string/preferencesNotificationLocation" />
    </androidx.preference.PreferenceCategory>
    <androidx.preference.PreferenceCategory
        app:iconSpaceReserved="false"
        app:title="@string/preferencesCategoryNotificationBackground">
        <androidx.preference.SwitchPreferenceCompat
            app:iconSpaceReserved="false"
            app:key="notificationEvents"
            app:summary="@string/preferencesNotificationEventsSummary"
            app:title="@string/preferencesNotificationEvents" />
    </androidx.preference.PreferenceCategory>
    <androidx.preference.PreferenceCategory
        app:iconSpaceReserved="false"
        app:title="@string/preferencesCategoryNotificationErrors">
        <androidx.preference.SwitchPreferenceCompat
            app:iconSpaceReserved="false"
            app:key="notificationGeocoderErrors"
            app:summary="@string/preferencesNotificationGeocoderErrorsSummary"
            app:title="@string/preferencesNotificationGeocoderErrors" />
    </androidx.preference.PreferenceCategory>
</PreferenceScreen>
